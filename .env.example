# Cjspanel Production Environment Variables
# Copy this file to .env and fill in your values

# REQUIRED: Random secret key for session encryption
# Generate with: python -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=your_secret_key_here

# SUPABASE CONFIGURATION
# Found in Project Settings > API
SUPABASE_URL=https://jblrvyhjeqsmdxuyjobs.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# DATABASE CONFIGURATION
# Found in Project Settings > Database > Connection String > URI
# Replace [YOUR-PASSWORD] with your actual database password
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.jblrvyhjeqsmdxuyjobs.supabase.co:5432/postgres

# STORAGE CONFIGuration
STORAGE_BUCKET=cjspanel-data

# ADMIN CREDENTIALS
ADMIN_USERNAME=Chetancj
ADMIN_PASSWORD=YourSecurePassword123

# FRONTEND CONFIGURATION
# The URL of your Netlify frontend (used for CORS and redirect settings)
ALLOWED_ORIGINS=https://your-app.netlify.app,http://localhost:3000

# PRODUCTION FLAG
PRODUCTION=true
